{
  "manifest_version": 2,
  "name": "xCurrency",
  "version": "1.2.5",
  "description": "An easy to use currency convertor that looks up multiple currencies at the same time instantly.",
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "currency.html"
  },
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "https://query.yahooapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self'"
}
